@using EntityLayer.Concrete
@model List<Contact>
<div class="col-md-6" data-aos="fade-left" data-aos-delay="300">
    @foreach (var x in Model)
    {
        <div class="mt-3 px-1">

            <div class="h5"></div>
            <p>@x.Title</p>
            <p>@x.Description</p>
            <p>Görüşmek üzere!</p>
        </div>
        <div class="mt-53 px-1">
            <div class="row">
                <div class="col-sm-2">
                    <div class="pb-1">Email:</div>
                </div>
                <div class="col-sm-10">
                    <div class="pb-1 fw-bolder">@x.Mail</div>
                </div>
                <div class="col-sm-2">
                    <div class="pb-1">Linkedin:</div>
                </div>
                <div class="col-sm-10">
                    <div class="pb-1 fw-bolder">https://www.linkedin.com/in/kerem-durak-0a4758183/</div>
                </div>
                <div class="col-sm-2">
                    <div class="pb-1">Telefon:</div>
                </div>
                <div class="col-sm-10">
                    <div class="pb-1 fw-bolder">@x.Phone</div>
                </div>
            </div>
        </div>
    }
</div>