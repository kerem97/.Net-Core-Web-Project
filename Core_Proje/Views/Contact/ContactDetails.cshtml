@model EntityLayer.Concrete.Message
@{
    ViewData["Title"] = "ContactDetails";
    Layout = "~/Views/Shared/_NewAdminLayOut.cshtml";
}




<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <p class="card-description"> Mesaj Detayları </p>
            <form class="forms-sample" method="post">
                <div class="form-group">
                    <label>ID</label>
                    @Html.TextBoxFor(x => x.MessageID, new { @class = "form-control" })
                </div>
                <div class="form-group">
                    <label>Başlık</label>
                    @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
                </div>
                <div class="form-group">
                    <label>Tarih</label>
                    @Html.TextBoxFor(x => x.Mail, new { @class = "form-control" })
                </div>
                <div class="form-group">
                    <label>Görsel URL</label>
                    @Html.TextBoxFor(x => x.Date, new { @class = "form-control" })
                </div>
                <div class="form-group">
                    <label>Açıklama</label>
                    @Html.TextAreaFor(x => x.Content, 6, 3, new { @class = "form-control" })
                </div>
                <a asp-action="Index" class="btn btn-primary mr-2">İletişim Sayfası</a>

            </form>
        </div>
    </div>
</div>































<script src="~/corona-free-dark-bootstrap-admin-template-1.0.0/template/assets/vendors/js/vendor.bundle.base.js"></script>
